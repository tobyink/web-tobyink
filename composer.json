{
	"require": {
	        "johnpbloch/wordpress": ">=5.5",
		"wpackagist-theme/twentytwenty": "*",
		"wpackagist-plugin/advanced-custom-fields": "*",
		"wpackagist-plugin/widget-logic": "*",
		"wpackagist-plugin/classic-editor": "*",
		"wpackagist-plugin/breadcrumb-navxt": "*",
		"wpackagist-plugin/foogallery": "*",
                "tobyink/foogallery-bootstrapped-gallery-template": "*",
                "tobyink/tobyink-contact": "*"
	},
	"extra": {
	        "installer-paths": {
	            "wp-content/plugins/{$name}/": [
	                "type:wordpress-plugin"
	            ],
	            "wp-content/themes/{$name}/": [
	                "type:wordpress-theme"
	            ]
	        },
	        "wordpress-install-dir": "wordpress"
	},
	"repositories": [
	        {
	            "type": "composer",
	            "url": "https://wpackagist.org"
	        },
                {
                    "type": "package",
                    "package": {
                        "name": "tobyink/foogallery-bootstrapped-gallery-template",
                        "version": "1.0.1",
                        "type": "wordpress-plugin",
                        "source": {
                            "url": "git@github.com:tobyink/php-wp-foogallery-bootstrapped-gallery-template.git",
                            "type": "git",
                            "reference": "master"
                        }
                    }
                },
                {
                    "type": "package",
                    "package": {
                        "name": "tobyink/tobyink-contact",
                        "version": "1.1",
                        "type": "wordpress-plugin",
                        "source": {
                            "url": "git@github.com:tobyink/php-wp-tobyink-contact.git",
                            "type": "git",
                            "reference": "master"
                        }
                    }
                }
	]
}
